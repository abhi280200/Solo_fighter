#include <bits/stdc++.h>
using namespace std;
#ifdef ONLINE_JUDGE
#define debug(...)
#else
#include "competitive/library/debug.cpp"
#endif
#define ll long long

int main(){
	int t; cin>>t;
	while (t--){
		int n; cin>>n;
		vector<int>b(n);
		vector<int>p(n);
		for (auto &a:b)cin>>a,a--;
		for (auto &a:p)cin>>a,a--;
		vector<int>dis(n,-1);
		dis[p[0]]=0;
		bool ans = true;
		int limit = 0;
		for (int i=1; i<n; i++){
			if (dis[b[p[i]]]==-1)ans=true;
			dis[p[i]] = limit+1;
			limit = dis[p[i]];
		}
		if (ans){
			for (auto d:dis)cout<<d<<" ";
			cout<<endl;
		}else{
			cout<<-1<<endl;
		}
	}
}
